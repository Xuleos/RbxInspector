local t = TS.import(script, TS.getModule(script, "t").lib.ts);
local APIDumpInterface = t.interface({
	Classes = t.table;
});
local function checkAPIDump(decodedAPIDump)
	return APIDumpInterface(decodedAPIDump);
end;
local fetchAPIDump = TS.async(function()
	local rawAPIDump = HttpService:GetAsync("https://raw.githubusercontent.com/CloneTrooper1019/Roblox-Client-Tracker/roblox/API-Dump.json");
	local decoded = HttpService:JSONDecode(rawAPIDump);
	if checkAPIDump(decoded) then
		return decoded;
	else
		error("APIDump failed quick check");
	end;
end);
exports.checkAPIDump = checkAPIDump;
exports.fetchAPIDump = fetchAPIDump;
return exports;
