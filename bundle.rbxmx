          <string name="Source"><![CDATA[-- Compiled with https://roblox-ts.github.io v0.3.1
-- February 7, 2020, 7:39 PM Eastern Standard Time

local TS = require(script.Parent.Parent.include.RuntimeLib);
local exports = {};
local Roact = TS.import(script, TS.getModule(script, "roact").roact.src);
local Dumpster = TS.import(script, TS.getModule(script, "dumpster").Dumpster).Dumpster;
local List = TS.import(script, script.Parent, "list").default;
local PropertyCard = TS.import(script, script.Parent, "BuiltInCards", "propertyCard");
local fetchAPIDump = TS.import(script, script.Parent.Parent, "APIDump", "RobloxAPI").fetchAPIDump;
local Selection = game:GetService("Selection");
local CACHE_DURATION = 7 * 86400;
local function isPluginSettings(pluginSettings)
	return typeof(pluginSettings) == "table";
end;
local App;
do
	App = Roact.PureComponent:extend("App");
	function App:init(p)
		self.dumpster = Dumpster.new();
	end;
	function App:render()
		local theme = settings().Studio.Theme;
		local cards;
		if self.state.Instance then
			cards = { Roact.createElement(
				PropertyCard,
				{
					widget = self.props.widget,
				}
			) };
		else
			cards = {};
		end;
		return Roact.createElement(
			"Frame",
			{
				BackgroundColor3 = theme:GetColor(Enum.StudioStyleGuideColor.MainBackground),
				BorderColor3 = theme:GetColor(Enum.StudioStyleGuideColor.Border),
				BorderSizePixel = 1,
				Size = UDim2.new(1, 0, 1, 0),
				Visible = true,
			},
			{
				Roact.createElement(
					List,
					{},
					cards
				),
			}
		);
	end;
	function App:didMount()
		self:setState({
			Instance = Selection:Get()[1];
		});
		self.dumpster:dump(Selection.SelectionChanged:Connect(function()
			print("Selection changed");
			self:setState({
				Instance = Selection:Get()[1];
			});
		end));
		local pluginSettings = self.props.plugin:GetSetting("RbxInspector");
		print("Getting API Dump");
		if pluginSettings == nil then
			fetchAPIDump():andThen(function(apiDump)
				local settings = {
					lastCacheTime = tick();
					APIDump = apiDump[1];
				};
				self.props.plugin:SetSetting("RbxInspector", settings);
			end):catch(function(err)
				error("Failed to get API dump: " .. err);
			end);
		elseif isPluginSettings(pluginSettings) then
			if tick() - pluginSettings.lastCacheTime >= CACHE_DURATION then
				local settings = TS.Object_deepCopy(pluginSettings);
				fetchAPIDump():andThen(function(apiDump)
					settings.APIDump = apiDump[1];
					settings.lastCacheTime = tick();
					self.props.plugin:SetSetting("RbxInspector", settings);
				end);
			end;
		else
			error("Plugin settings is incorrectly formatted. Please forward this to the developer @Xuleos_");
		end;
		print(self.props.plugin:GetSetting("RbxInspector"));
	end;
	function App:willUnmount()
		self.dumpster:burn();
	end;
end;
exports.default = App;
return exports;
]]></string>
